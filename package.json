{
  "name": "user-office-scheduler-frontend",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/UserOfficeProject/user-office-scheduler-frontend.git"
  },
  "private": true,
  "scripts": {
    "dev": "react-scripts start",
    "build": "react-scripts --max_old_space_size=4096 build",
    "eject": "react-scripts eject",
    "lint": "tsc --noEmit && eslint . --ext .js,.jsx,.ts,.tsx --quiet",
    "lint:all": "tsc --noEmit && eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "tsc --noEmit && eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "generate:local": "cp codegen_template.yml codegen_tmp.yml && graphql-codegen -c codegen_tmp.yml && rm ./codegen_tmp.yml",
    "cy:open": "cypress open",
    "cy:run": "cypress run"
  },
  "dependencies": {
    "@date-io/moment": "^1.3.13",
    "@esss-swap/duo-localisation": "^1.1.15",
    "@esss-swap/duo-validation": "^1.1.42",
    "@graphql-codegen/cli": "^1.19.4",
    "@graphql-codegen/typescript": "^1.19.0",
    "@graphql-codegen/typescript-graphql-request": "^2.0.3",
    "@graphql-codegen/typescript-operations": "^1.17.12",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/pickers": "^3.2.10",
    "@types/dateformat": "^3.0.1",
    "@types/humanize-duration": "^3.18.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.165",
    "@types/react": "^16.14.2",
    "@types/react-big-calendar": "^0.24.8",
    "@types/react-dom": "^16.9.10",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/yup": "^0.29.10",
    "date-fns": "^2.16.1",
    "dateformat": "^3.0.3",
    "formik": "^2.2.5",
    "formik-material-ui": "^3.0.1",
    "formik-material-ui-pickers": "0.0.11",
    "graphql": "^15.4.0",
    "graphql-request": "^3.3.0",
    "humanize-duration": "^3.24.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "notistack": "^1.0.2",
    "react": "^16.14.0",
    "react-big-calendar": "^0.28.6",
    "react-cookie": "^4.0.3",
    "react-dom": "^16.14.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "typescript": "^3.9.7",
    "yup": "^0.29.3"
  },
  "lint-staged": {
    "**/*.{js,ts}": "npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/eslint": "~6.8.1",
    "@types/jest": "24.0.16",
    "@types/node": "^12.19.8",
    "@typescript-eslint/eslint-plugin": "~2.34.0",
    "@typescript-eslint/parser": "~2.34.0",
    "cypress": "6.6.0",
    "eslint": "~6.8.0",
    "eslint-config-prettier": "~6.11.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "~3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "~2.5.1",
    "http-proxy-middleware": "^1.0.5",
    "husky": "~4.2.5",
    "lint-staged": "^10.5.3",
    "prettier": "~1.19.1"
  }
}
